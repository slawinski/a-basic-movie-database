<div>
  <a href="/">
    <button class="btn btn-outline-secondary">Back</button>
  </a>
</div>
{{#if movies}} {{#each movies}}
<a href="/movies/{{id}}">
  <div class="card" style="width: 18rem;">
    <div class="card-body">
      <h5 class="card-title">{{Title}} ({{Year}})</h5>
      <h6 class="card-subtitle mb-2 text-muted">{{Genre}}</h6>
    </div>
  </div>
</a>
{{/each}} {{else}}
<div class="alert alert-danger">There should be something here</div>
{{/if}}
