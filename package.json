{
  "name": "a-basic-movie-database",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "webpack --mode development ./src/index.js --output ./public/javascripts/main.js",
    "build": "webpack --mode production ./src/index.js --output ./public/javascripts/main.js",
    "watch": "webpack -w --mode development",
    "start": "NODE_ENV=production nodemon ./bin/www",
    "postinstall": "knex migrate:latest && npm install"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-handlebars": "^3.0.0",
    "hbs": "~4.0.1",
    "http-errors": "~1.6.2",
    "knex": "^0.15.2",
    "method-override": "^3.0.0",
    "morgan": "~1.9.0",
    "node-fetch": "^2.1.2",
    "pg": "^7.5.0",
    "pg-promise": "^8.4.5"
  },
  "devDependencies": {
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "nodemon": "^1.17.5",
    "prettier": "^1.14.3"
  }
}
